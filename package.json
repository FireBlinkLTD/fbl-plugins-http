{
  "name": "@fbl-plugins/http",
  "version": "1.0.2",
  "description": "fbl plugin to make http requests",
  "main": "dist/index.js",
  "engines": {
    "node": ">=8.12.0"
  },
  "scripts": {
    "fbl": "fbl",
    "tsc": "tsc",
    "rimraf": "rimraf",
    "depcheck": "yarn-check",
    "start": "node dist/src/cli.js",
    "test": "nyc --reporter=html --reporter=text mocha",
    "build": "rimraf ./dist/ && tsc",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'test/**/*.ts'",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "author": "FireBlink LTD",
  "license": "MIT",
  "dependencies": {
    "form-data": "2.3.3",
    "got": "9.4.0",
    "joi": "14.3.0",
    "mime-types": "2.1.21",
    "stream-buffers": "3.0.2",
    "typedi": "0.8.0",
    "user-agents": "1.0.102"
  },
  "peerDependencies": {
    "fbl": ">=1.3.0 <2.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.17.0",
    "@types/chai": "4.1.7",
    "@types/express": "4.16.0",
    "@types/form-data": "2.2.1",
    "@types/got": "9.2.1",
    "@types/joi": "14.0.0",
    "@types/mime-types": "2.1.0",
    "@types/multiparty": "0.0.31",
    "@types/node": "10.12.12",
    "@types/stream-buffers": "3.0.2",
    "body-parser": "1.18.3",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "express": "4.16.4",
    "fbl": "1.3.1",
    "husky": "1.2.0",
    "mocha": "5.2.0",
    "mocha-typescript": "1.1.17",
    "mochawesome": "3.1.1",
    "multiparty": "4.2.1",
    "nyc": "13.1.0",
    "rimraf": "2.6.2",
    "source-map-support": "0.5.9",
    "tslint": "5.11.0",
    "typescript": "3.2.2",
    "yarn-check": "0.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn outdated"
    }
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "exclude": [
      "dist/src/**/index.js",
      "dist/index.js",
      "dist/test/**/*.js"
    ]
  },
  "contributors": [
    {
      "name": "Vladyslav Tkachenko",
      "email": "vlad@fireblink.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FireBlinkLTD/fbl-plugins-http.git"
  },
  "keywords": [
    "fireblink",
    "fbl",
    "plugin",
    "plugins",
    "rest",
    "http",
    "https",
    "client",
    "cli",
    "automation",
    "upload",
    "download"
  ]
}
