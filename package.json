{
  "name": "@fbl-plugins/http",
  "version": "1.1.0",
  "description": "fbl plugin to make http requests",
  "main": "dist/index.js",
  "engines": {
    "node": ">=8.12.0"
  },
  "scripts": {
    "commit": "npx git-cz",
    "commit:retry": "npx git-cz --retry",
    "depcheck": "yarn-check",
    "start": "node dist/src/cli.js",
    "test": "nyc --reporter=html --reporter=text mocha",
    "build": "rimraf ./dist/ && tsc",
    "lint": "yarn outdated && tslint -c tslint.json 'src/**/*.ts' 'test/**/*.ts'",
    "prettify": "prettier src/**/*.{md,ts,json} test/**/*.{md,ts,json} index.{md,ts,json} --write --config .prettierrc.yml && tslint 'src/**/*.ts' 'test/**/*.ts' 'index.ts' --fix",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "author": "FireBlink LTD",
  "license": "MIT",
  "dependencies": {
    "form-data": "2.3.3",
    "got": "9.6.0",
    "joi": "14.3.1",
    "mime-types": "2.1.22",
    "stream-buffers": "3.0.2",
    "typedi": "0.8.0",
    "user-agents": "1.0.210"
  },
  "peerDependencies": {
    "fbl": ">=1.7.0 <2.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "7.5.2",
    "@commitlint/config-conventional": "7.5.0",
    "@types/body-parser": "1.17.0",
    "@types/chai": "4.1.7",
    "@types/express": "4.16.1",
    "@types/form-data": "2.2.1",
    "@types/got": "9.4.1",
    "@types/joi": "14.3.2",
    "@types/mime-types": "2.1.0",
    "@types/multiparty": "0.0.32",
    "@types/node": "11.12.1",
    "@types/stream-buffers": "3.0.2",
    "body-parser": "1.18.3",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "commitizen": "3.0.7",
    "express": "4.16.4",
    "fbl": "1.7.0",
    "husky": "1.3.1",
    "jsonlint": "1.6.3",
    "lint-staged": "8.1.5",
    "mocha": "6.0.2",
    "mocha-typescript": "1.1.17",
    "mochawesome": "3.1.1",
    "multiparty": "4.2.1",
    "nyc": "13.3.0",
    "prettier": "1.16.4",
    "rimraf": "2.6.3",
    "source-map-support": "0.5.12",
    "tslint": "5.14.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.3.4000",
    "yarn-check": "0.0.3"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "exclude": [
      "dist/src/**/index.js",
      "dist/index.js",
      "dist/test/**/*.js"
    ]
  },
  "contributors": [
    {
      "name": "Vladyslav Tkachenko",
      "email": "vlad@fireblink.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FireBlinkLTD/fbl-plugins-http.git"
  },
  "keywords": [
    "fireblink",
    "fbl",
    "plugin",
    "plugins",
    "rest",
    "http",
    "https",
    "client",
    "cli",
    "automation",
    "upload",
    "download"
  ]
}
